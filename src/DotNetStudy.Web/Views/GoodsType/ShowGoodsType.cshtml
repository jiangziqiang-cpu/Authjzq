
@{
    ViewData["Title"] = "ShowGoodsType";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}
@model IEnumerable<DotNetStudy.Web.Models.GoodsType>
<h1>商品分类</h1>
<a class="btn btn-primary" href="/GoodsType/AddGoodsType" role="button">添加分类</a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">编号ID</th>
            <th scope="col">上一级ID</th>
            <th scope="col">分类名称</th>
            <th scope="col">操作</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in Model)
            {
            <tr>
                <th scope="row">@item.GoodsTypeId</th>
                <td>@item.HigherLevelId</td>
                <td>@item.GoodsTypeName</td>
                <td>@Html.ActionLink("更新分类", "UpdateGoodsType", new { id = item.GoodsTypeId })</td>
            </tr>



            }

        }
    </tbody>
</table>
<div>
    <a href="/Goods/ShowGoods">返回</a>
</div>
